{
  "name": "games",
  "version": "1.0.0",
  "main": "server/index.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server": "nodemon ./server/index.ts",
    "start": "DATABASE_URL=postgres://postgres:postgres@localhost:5432/crypto_games npm run server",
    "build": "webpack --config webpack.config.js --color --verbose --progress --watch",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "typeorm": "ts-node -r ./ormconfig.ts ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm:cli -- migration:generate --connection  --name ",
    "migration:run": "npm run typeorm:cli -- migration:run",
    "migration:revert": "npm run typeorm:cli -- migration:revert"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "axios": "^0.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^1.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2",
    "vue": "^2.6.10",
    "vue-cookies": "^1.5.13",
    "vue-i18n": "^8.15.0",
    "vue-loader": "^15.7.2",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@types/morgan": "^1.7.37",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "pg": "^7.12.1",
    "typeorm": "^0.2.20"
  }
}
